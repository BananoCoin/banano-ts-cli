{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "instrument": true,
  "reporter": [
    "html",
    "text",
    "text-summary"
  ],
  "extension": [
    ".ts"
  ],
  "include": [
    "**/*.js",
    "**/*.ts"
  ],
  "exclude": [
    "coverage/**",
    "node_modules/**",
    "**/*.d.ts",
    "**/*.test.ts",
    ".eslintrc.js",
    "documentation.js",
    "src/main.ts"
  ]
}
